    {
      "cell_type": "code",
      "metadata": {
        "colab": {
          "base_uri": "https://localhost:8080/"
        },
        "id": "385f39b6",
        "outputId": "14dbdcdc-89f5-4975-f7d3-c6305c17cc58"
      },
      "source": [
        "# Define the zip file path (assuming it's in /content/ after upload)\n",
        "zip_file_path = '/content/archive (3).zip'\n",
        "\n",
        "# Define the destination directory for extraction (your base data directory)\n",
        "destination_dir = '/content/drive/MyDrive/cheetah vs hyaena'\n",
        "\n",
        "import os\n",
        "import zipfile\n",
        "\n",
        "print(f\"Unzipping {zip_file_path} to {destination_dir}...\")\n",
        "\n",
        "if os.path.exists(zip_file_path):\n",
        "    try:\n",
        "        with zipfile.ZipFile(zip_file_path, 'r') as zip_ref:\n",
        "            zip_ref.extractall(destination_dir)\n",
        "        print(\"Extraction complete.\")\n",
        "    except zipfile.BadZipFile:\n",
        "        print(f\"Error: {zip_file_path} is not a valid zip file.\")\n",
        "    except Exception as e:\n",
        "        print(f\"An error occurred during extraction: {e}\")\n",
        "else:\n",
        "    print(f\"Error: Zip file not found at {zip_file_path}\")\n",
        "\n",
        "# Now, let's verify the contents of the destination directory\n",
        "print(f\"\\nContents of {destination_dir} after extraction:\")\n",
        "if os.path.exists(destination_dir):\n",
        "    for item in os.listdir(destination_dir):\n",
        "        print(f\"  {item}\")\n",
        "else:\n",
        "    print(f\"Error: Destination directory not found: {destination_dir}\")\n",
        "\n",
        "# We need to correctly identify the train and test directories after extraction.\n",
        "# The zip file might have created an extra directory level (e.g., /base_dir/dataset_name/train).\n",
        "# Let's try to find the actual 'train' and 'test' folders.\n",
        "\n",
        "found_train_path = None\n",
        "found_test_path = None\n",
        "\n",
        "for root, dirs, files in os.walk(destination_dir):\n",
        "    if 'train' in dirs and found_train_path is None:\n",
        "        # Check if it's a direct child of the dataset root\n",
        "        if os.path.basename(root) == os.path.basename(destination_dir) or \\\n",
        "           (os.path.basename(root) != os.path.basename(destination_dir) and \\\n",
        "            'cheetah' in os.listdir(os.path.join(root, 'train')) and \\\n",
        "            'hyena' in os.listdir(os.path.join(root, 'train'))):\n",
        "            found_train_path = os.path.join(root, 'train')\n",
        "    if 'test' in dirs and found_test_path is None:\n",
        "        # Check if it's a direct child of the dataset root\n",
        "        if os.path.basename(root) == os.path.basename(destination_dir) or \\\n",
        "           (os.path.basename(root) != os.path.basename(destination_dir) and \\\n",
        "            'cheetah' in os.listdir(os.path.join(root, 'test')) and \\\n",
        "            'hyena' in os.listdir(os.path.join(root, 'test'))):\n",
        "            found_test_path = os.path.join(root, 'test')\n",
        "    if found_train_path and found_test_path:\n",
        "        break\n",
        "\n",
        "# Fallback if specific subdirectories like 'cheetah'/'hyena' are not immediately found\n",
        "if not found_train_path:\n",
        "    for root, dirs, files in os.walk(destination_dir):\n",
        "        if 'train' in dirs and 'cheetah' in os.listdir(os.path.join(root, 'train')):\n",
        "            found_train_path = os.path.join(root, 'train')\n",
        "            break\n",
        "\n",
        "if not found_test_path:\n",
        "    for root, dirs, files in os.walk(destination_dir):\n",
        "        if 'test' in dirs and 'cheetah' in os.listdir(os.path.join(root, 'test')):\n",
        "            found_test_path = os.path.join(root, 'test')\n",
        "            break\n",
        "\n",
        "# Update global variables for train_dir and test_dir\n",
        "if found_train_path:\n",
        "    global train_dir\n",
        "    train_dir = found_train_path\n",
        "    print(f\"\\nUpdated train_dir: {train_dir}\")\n",
        "    print(f\"Number of items in train_dir: {len(os.listdir(train_dir))}\")\n",
        "else:\n",
        "    print(\"\\nCould not find 'train' directory after extraction. Please check the zip file structure.\")\n",
        "\n",
        "if found_test_path:\n",
        "    global test_dir\n",
        "    test_dir = found_test_path\n",
        "    print(f\"Updated test_dir: {test_dir}\")\n",
        "    print(f\"Number of items in test_dir: {len(os.listdir(test_dir))}\")\n",
        "else:\n",
        "    print(\"Could not find 'test' directory after extraction. Please check the zip file structure.\")\n",
        "\n",
        "\n",
        "# Re-create the test_data generator with the updated test_dir if found\n",
        "from tensorflow.keras.preprocessing.image import ImageDataGenerator\n",
        "\n",
        "if found_test_path:\n",
        "    test_datagen = ImageDataGenerator(rescale=1./255)\n",
        "    test_data = test_datagen.flow_from_directory(\n",
        "        test_dir,\n",
        "        target_size=(256,256),\n",
        "        batch_size=32,\n",
        "        class_mode='binary'\n",
        "    )\n",
        "    print(f\"\\nNumber of samples found in test_data generator: {test_data.n}\")\n",
        "    print(f\"Class labels: {test_data.class_indices}\")\n",
        "else:\n",
        "    print(\"Cannot recreate test_data generator: test_dir was not found.\")"
      ],
